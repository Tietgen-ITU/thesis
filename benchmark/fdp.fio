[global]
filename=${DEVICE}
iodepth=${IODEPTH}
bs=${BS}
numjobs=${THREADS}
ioengine=io_uring_cmd
time_based=1
runtime=6h
group_reporting
fdp=1

[write-heavy]
rw=randrw
rwmixwrite=90
fdp_pli=0,1,2,3
offset=0%
size=30%
flow_id=42
flow=1

[write-mid]
rw=randrw
rwmixwrite=30
fdp_pli=4,5
offset=30%
size=30%
flow_id=42
flow=1

[write-light]
rw=randrw
rwmixwrite=10
fdp_pli=6
offset=60%
size=30%
flow_id=42
flow=1