[global]
filename=/dev/ng1n1
ioengine=xnvme
xnvme_async=io_uring_cmd
iodepth=32
fdp=1
time_based=1
runtime=18000
norandommap=1
randrepeat=1
numjobs=1
thread=1

[seq1]
bs=128k
rw=write
offset=0%
size=45%
fdp_pli=0
rate=256m,256m

[rand1]
bs=4k
rate=256m,256m
rw=randwrite
offset=45%
size=5%
fdp_pli=1

[seq2]
bs=128k
rw=write
offset=50%
size=45%
fdp_pli=2
rate=256m,256m

[rand2]
bs=4k
rate=256m,256m
rw=randwrite
offset=95%
size=5%
fdp_pli=3
