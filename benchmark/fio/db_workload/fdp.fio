[global]
filename=${DEVICE}
iodepth=${IODEPTH}
bs=256ki
numjobs=${THREADS}
ioengine=io_uring_cmd
fdp=1
time_based=1
runtime=${DURATION}
group_reporting
norandommap=1

[database]
rw=randrw
rwmixwrite=10%
percentage_random=90,0
fdp_pli=0,1,2,3,4
offset=0%
size=93%
rate=310mi,310mi
new_group

[writeahead]
rw=write
fdp_pli=5
offset=94%
size=1%
rate=,5mi
new_group

[temporary]
rw=randrw
fdp_pli=6
offset=95%
size=5%
rate=20mi,20mi
new_group
