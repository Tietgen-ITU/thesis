[global]
filename=${DEVICE}
iodepth=${IODEPTH}
bs=256ki
numjobs=${THREADS}
ioengine=xnvme
xnvme_async=io_uring_cmd
time_based=1
thread=1
runtime=${DURATION}
group_reporting
norandommap=1

[database]
rw=randrw
rwmixwrite=10%
offset=0%
size=93%
rate=310mi,310mi
new_group

[writeahead]
rw=write
offset=94%
size=1%
rate=5mi,5mi
new_group

[temporary]
rw=randrw
offset=95%
size=5%
rate=20mi,20mi
new_group
