[global]
filename=${DEVICE}
iodepth=${IODEPTH}
bs=${BS}
numjobs=${THREADS}
ioengine=xnvme
xnvme_async=io_uring_cmd
time_based=1
runtime=6h
group_reporting
thread=1


[write-heavy]
rw=randrw
rwmixwrite=90
offset=0%
size=30%
new_group

[write-mid]
rw=randrw
rwmixwrite=30
offset=30%
size=30%
new_group

[write-light]
rw=randrw
rwmixwrite=10
offset=0%
size=30%
new_group
