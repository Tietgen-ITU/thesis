[global]
filename=${DEVICE}
iodepth=${IODEPTH}
bs=${BS}
numjobs=${THREADS}
ioengine=xnvme
xnvme_async=io_uring_cmd
time_based=1
runtime=6h
output-format=json
output=${OUTPUT}
group_reporting


[write-heavy]
rw=randrw
rwmixwrite=90
size=30%

[write-mid]
rw=randrw
rwmixwrite=30
size=30%

[write-light]
rw=randrw
rwmixwrite=10
size=30%